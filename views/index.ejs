<div class="author">
    <h1 class="author__heading">Hello</h1>
    <p class="author__body">Developer</p>
</div>
<img src="/img/image1.png" alt="author" class="author__img" width="650" height="528">
<section class="articles">
    <h2 class="articles__heading">LatestPosts</h2>
    <ul class="articles__ul">
        <% data.forEach(post => { %>
         
        <li>
            <a href="/post/"<%= post._id %> >
                <span><%= post.title%></span>
                <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
            </a>
        </li>
        <% }) %>
    </ul>

    <% if (nextPage !== null) { %>
        <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts </a>
    <% } %>
</section>